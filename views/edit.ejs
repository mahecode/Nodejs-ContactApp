<% include ../views/partials/header %>
<div class="container">

<div class="row">
  <form class="col s6" action="/edit/<%= doc._id %>"  method="post">
    <div class="input-field col s6">
      <input id="last_name" type="text" class="validate" value="<%= doc.name %>" name="name">
      <label for="last_name">Name</label>
    </div>
    <div class="input-field col s6">
      <input id="first_name" type="text" class="validate" value="<%= doc.mo_no %>" name="number">
      <label for="first_name">Number</label>
    </div>
      <button class="btn waves-effect waves-light" type="submit">Done</button>
      <button class="btn waves-effect waves-light" type="submit">Cancel</button>
  </form>
</div>
</div>
<% include ../views/partials/footer %>
