<% include ../views/partials/header %>
<div class="container">
    <div class="col s6">
    <table class="striped">
      <thead>
        <tr>
          <th>Name</th>
          <th>Phone Number</th>
        </tr>
        <tbody>
          <% contacts.forEach(function(contacts){ %>
            <tr>
              <td><strong><%= contacts.name %></strong></td>
              <td><strong><%= contacts.mo_no %></strong></td>
              <td><a class="waves-effect waves-light btn btn-small " href="/edit/<%= contacts.id %>">Edit</a> <a class="delete-contact waves-effect waves-light btn btn-small red darken-2" href="#" data-id="<%= contacts.id %>">Delete</a> </td>
            </tr>
          <% }) %>
        </tbody>
      </thead>
    </table>
   </div>
 </div>
</div>
<% include ../views/partials/footer %>
